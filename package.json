{
  "name": "volatility-swap",
  "version": "1.0.0",
  "description": "Algorithmic stablecoin system with a market-determined collateralization ratio and a corresponding unstablecoin leveraged asset.",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test --compile-all",
    "migrate-goerli": "truffle migrate --network goerli",
    "migrate-goerli-dry-run": "truffle migrate --network goerli --dry-run",
    "verify-goerli": "truffle run verify Swap Token Timelock --network goerli",
    "compile": "truffle compile --all",
    "clean": "truffle networks --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonahGroendal/volatility-swap.git"
  },
  "keywords": [
    "stablecoin",
    "unstablecoin",
    "algorithmic",
    "leverage",
    "leveraged",
    "ETH",
    "ERC20",
    "solidity",
    "ethereum"
  ],
  "author": "Jonah Groendal",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/JonahGroendal/volatility-swap/issues"
  },
  "homepage": "https://github.com/JonahGroendal/volatility-swap#readme",
  "dependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/truffle-upgrades": "^1.16.0",
    "@truffle/hdwallet-provider": "^2.0.14",
    "truffle": "^5.5.29"
  },
  "devDependencies": {
    "eth-gas-reporter": "^0.2.24",
    "openzeppelin-test-helpers": "^0.5.1",
    "truffle-plugin-verify": "^0.5.28"
  }
}
